alias aar='sudo add-apt-repository' # add package manager repo, requires ppa 
alias aliases='~/.io/alias' # All aliases + Descriptions in less / just aliases matching argument
alias aptlist='apt list --upgradable' # list apt upgrades
alias archv='$HOME/.io/Archive' # Archive $files $ArchiveName
alias autorm='sudo apt auto-remove -y' # Cleanup removed apps
alias brackets='$HOME/.io/gtab brackets' # run in new tab
alias brave='$HOME/.io/Brave' # Well titled Brave browser
alias ..='cd ../' # Up 1
alias cleanpass='rm $HOME/Templates/Database.kdbx' # Clear KPDB
alias cls='clear && date | lolcat' # Clear and Date
alias disks='gnome-disks' # Yeah What?!
alias Download='$HOME/.io/Download' # Send pasted URL to Downloads folder
alias EAL='sudo nano $HOME/.io/config/.aliases' # Verify user to Edit Aliases
alias gaa='git add --all' # Thats pretty verbose
alias gcam='git commit -a -m' # commit all supply message on CLI
alias gcan='git commit -a' # Git commit all NANO the commit msg
alias gcc='$HOME/.io/git.CLONE' # Git clone practice for gitclone copy to copy multiple dirs
alias gen.RM='$HOME/.io/gen.README' # Generate Markdown readme for a script
alias get.ports='sudo netstat -tunlp' # Show listening ports
alias gim='/home/lij/.io/gim' # Edit in Vim and then commit
alias gioc='git add ~/.io/config/.*' # Add hidden configs in .io/config
alias gio='$HOME/.io/nano.GITPUSH' # Edit in nano then push 
alias gits='git status' # Status of the local git project
alias gp='git pull' # Little is pull big is push
alias GP='git push' # Big is push little is pull
alias GPT='printf "Pulled at: " && stat -c %y .git/FETCH_HEAD' # Time of Most recent local git pull
alias gtab='$HOME/.io/gtab' # New Gnome-Terminal Tab
alias help="help  " # "don\'t forget to use 'help', and "<cmd> --help" if you need bash commands!"
alias hs='history | grep -n' # Search recent commands 
alias ?='~/.io/alias' # Alias help, see aliases v
alias ipl="printf \"Local IP: $(ip a | grep 10.245 | cut -f6 -d ' ') \n\"" # Local IP address
alias keepass='/usr/bin/keepass' # Password Vault
alias la='ls -ahF' # All with symbols
alias ll='ls -ahlF' # Perms List with Icons
alias marktext='/usr/bin/marktext.AppImage' # MarkText Notes
alias nabak='mkdir .naBak' # set nano
alias na='$HOME/.io/newAlias' # Create Alias
alias nano='nano -N' # Don't Convert mac/Dos 
alias nanorc?='ls ~/.io/config/nano/nanorc | grep nanorc' # Is there a vimrc file in the proper config directory?
alias nash='sudo nano $HOME/.ssh/config' # Configure SSH
alias purge='$HOME/.io/purge' # Package Manager Full Remover
alias q='exit' # quit
alias search='$HOME/.io/search' # Query for Files
