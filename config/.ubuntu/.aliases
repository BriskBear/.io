# alias steam='$HOME/.io/gtab steam' # 'Steam in new term-tab' 
alias steam='$HOME/.io/xtab steam' # 'Steam in new term-tab' 
alias sudo='sudo  ' # sudo enable .io/config/.aliases
alias SVC='sudo systemctl' # Service Control
alias tal='tree -aQFlL' # Tree + Hidden for x layers:: tal <x> [<path>]
alias tar.ext='tar -xjvf' # extract tbz2
alias tar.list='tar -i -tf' # List files in an appended tbz2 archive
alias ta='tree -aQlF' # Tree + hidden
alias update='sudo apt update -y && apt list --upgradeable | lolcat' # Update Package Manager
alias upgrade='sudo apt upgrade -y|lolcat' # Upgrade apt
alias vash='vim $HOME/.ssh/config' # Open .ssh/config in Vim Text Editor
alias vimrc?='ls ~/.io/config/vimrc/vimrc | grep vimrc' # Is there a vimrc file in the proper config directory?
alias +x='chmod -v +x' # Whoops
alias x='$HOME/.io/.sec/.x' # Secret secrets
alias xtab='$HOME/.io/xtab' # Open command in new XFCE Terminal tab
