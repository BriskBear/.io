alias setCFG='$HOME/.io/Prepdot' # Update .io Configs
alias setup='$HOME/.io/setup' # Package manager install; singular or space-separated list.
alias sna='sudo nano' # Root Text Edit
alias splt='$HOME/.io/split.string' # Split a string into an array
