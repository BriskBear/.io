alias aar='sudo add-apt-repository' # add package manager repo, requires ppa 
alias aptlist='apt list --upgradable' # list apt upgrades
alias archv='/home/lij/.io/Archive' # Archive $files $ArchiveName
alias autorm='sudo apt auto-remove -y' # Cleanup removed apps
alias brackets='~/.io/gtab brackets' # run in new tab
alias brave='~/.io/Brave' # Well titled Brave browser
alias cat='cat -n' # cat w/  line numbers
alias ?='cat ~/.io/.aliases' # Alias Help
alias ..='cd ..' # Up 1
